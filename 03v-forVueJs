<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>v-for</title>
    <!-- 
        v-for 循环遍历一个或多个对象
     -->
    <script src="../lib/vue.js"></script>
  </head>

  <body>
    <div id='axx'>
      <div>{{message}}</div>
      <ul>
        <li v-for="item in movies">{{item}}</li>
      </ul>
    </div>

    <script>
      const app = new Vue({
        el: '#axx',
        data: {
          message:'你好啊',
          movies:['星际穿越','哪吒','赵氏孤儿案','复仇者联盟']
        }
      })
    </script>
  </body>
</html>
