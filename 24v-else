<!DOCTYPE html>
<html>
  <head>
    <title>v-else</title>
    <script src="lib/vue.js"></script>
  </head>

  <body>
    <div id="axx">
      <h2 v-if='isShow'> 
        {{message}} 
        <div>1</div>
        <div>2</div>
        <div>3</div>
      </h2>
      <h1 v-else>isShow为false时显示</h1>
    </div>
    
    <script>
      const axx = new Vue({
        el: "#axx",
        data: {
          message: '嘿嘿嘿',
          isShow: true
        }
      })
    </script>
  </body>
</html>
